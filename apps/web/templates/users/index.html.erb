<%= breadcrumbs %>

<div class="overflow-scroll">
  <table class="table-light overflow-hidden rounded border h4">
    <thead class="bg-darken-1">
      <tr>
        <th class="center">
          <i class="fa fa-female"></i>
        </th>
        <th class="center">
          <i class="fa fa-child"></i>
        </th>
      </tr>
    </thead>
    <tbody>
      <% children.each do |child| %>
        <%= render partial: '/users/index_user', locals: {child: child, mother: child.mother} %>
      <% end %>
    </tbody>
  </table>
</div>
